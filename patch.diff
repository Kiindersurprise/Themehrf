diff --git a/css/smartnoti.style.css b/css/smartnoti.style.css
new file mode 100644
index 0000000..66a8725
--- /dev/null
+++ b/css/smartnoti.style.css
@@ -0,0 +1,212 @@
+#case_press{
+	position: fixed;
+	top: 10px;
+	right: 10px;
+	display: block;
+	list-style: none; 
+	padding: 0;
+	margin: 0;
+}
+#case_press [data-case]{
+	width: 240px;
+	position: relative;
+	top: 0;
+	right: 0;
+	margin-bottom: 10px;
+	padding: 45px 15px 10px 15px;
+	float: right;
+	clear: right;
+	font-size: 12px;
+	overflow: hidden;
+	-moz-box-shadow: 0 2px 5px rgba(0,0,0,0.5); /* Firefox */
+    -webkit-box-shadow: 0 2px 5px rgba(0,0,0,0.5); /* Safari and Chrome */
+    box-shadow: 0 2px 5px rgba(0,0,0,0.5);
+    background: #fff;
+    display: none;
+}
+#case_press [data-case] img{
+	float: left;
+	margin-right: 15px;
+	border: 0px none;
+}
+#case_press [data-case] iframe, #case_press [data-case] object, #case_press [data-case] embed{
+	width: 100%;
+	height: 200px;
+	max-height: 200px;
+	z-index: 1;
+}
+#case_press [data-close],#case_press [data-open],#case_press [data-toggle]{
+	color: #fff;
+	height: 35px;
+	line-height: 35px;
+}
+#case_press [data-close]:hover,#case_press [data-open]:hover,#case_press [data-toggle]:hover{
+	line-height: 34px;
+}
+#case_press [data-close]{
+	position: absolute;
+	top: 0;
+	right: 5px;
+	width: 20px;
+	text-align: center;
+	cursor: pointer;
+	font-weight: bold;
+	font-size: 12px;
+	visibility: visible;
+}
+#case_press [data-open]{
+	position: absolute;
+	top: 0;
+	right: 20px;
+	width: 20px;
+	text-align: center;
+	cursor: pointer;
+	font-weight: bold;
+	font-size: 12px;
+	visibility: visible;
+}
+#case_press [data-toggle]{
+	position: absolute;
+	top: 0;
+	right: 35px;
+	width: 20px;
+	text-align: center;
+	cursor: pointer;
+	font-weight: bold;
+	font-size: 12px;
+	visibility: visible;
+}
+#case_press [data-hidcont]{
+	display: none;
+	float: left;
+	width: 100%;
+	overflow: hidden;
+	margin-top: 10px;
+	overflow: auto;
+	scrollbar-3dlight-color:#fff; 
+	scrollbar-arrow-color:#000;
+	scrollbar-highlight-color: #fff;
+	scrollbar-face-color:#cbcbcb;
+	scrollbar-shadow-color:#fff;
+	scrollbar-darkshadow-color:#fff;
+	scrollbar-track-color: #fff;
+}
+#case_press [data-hidcont] img{
+	clear: both;
+
+}
+.clear-p{
+	padding-bottom: 0px;
+	padding-top: 0px;
+	visibility: hidden;
+}
+.clear-p iframe, .clear-p object, .clear-p embed {
+	visibility: hidden;
+}
+.auto_center{
+	position: fixed;
+	z-index: 999;
+	padding-top: 45px;
+	padding-bottom: 10px;
+	min-width: 200px;
+	visibility: visible;
+
+}
+.auto_center iframe, .auto_center object, .auto_center embed{
+	width: 100%;
+	height: 100%;
+	visibility: visible;
+	height: auto;
+	min-height: 350px;
+	display: block;
+}
+#general_overlay{
+	width: 100%;
+	height: 100%;
+	position: absolute;
+	top: 0;
+	left: 0;
+	background: #000;
+	display: none;
+}
+#case_press [data-ptop]{
+	background: #f4dc12;
+	color: white;
+	width: 100%;
+	height: 35px;
+	position: absolute;
+	top: 0;
+	right: 0;
+	text-align: center;
+	line-height: 35px;
+	font-style: italic;
+	font-size: 14px;
+	visibility: visible;
+}
+/* USER STYLES*/
+
+/* header the container notices */
+#case_press [data-ptop='note_1']{
+	background: #E7B621 url(img/mail.png) no-repeat;
+	background-position: 3% 50%;
+}
+#case_press [data-ptop='note_2']{
+	background: #4fcff4 url(img/camera.png) no-repeat;
+	background-position: 3% 50%;
+}
+#case_press [data-ptop='note_3']{
+	background: #8EC122 url(img/info.png) no-repeat;
+	background-position: 3% 50%;
+}
+#case_press [data-ptop='note_4']{
+	background: #212121 url(img/chat.png) no-repeat;
+	background-position: 3% 50%;
+}
+#case_press [data-ptop='note_5']{
+	background: #FF5866 url(img/info.png) no-repeat;
+	background-position: 3% 50%;
+}
+#case_press [data-ptop='note_6']{
+	background: #FF9A3A url(img/mail.png) no-repeat;
+	background-position: 3% 50%;
+}
+.link_1{
+	font-size: 14px;
+	line-height: 16px;
+	margin-top: 3px;
+	display: block;
+	font-style: italic;
+	color: #f4dc12;
+}
+
+/* container notices */
+#case_press [data-case='note_1']{
+	background: #fff;
+	width: 300px;
+}
+
+#case_press [data-case='note_2']{
+	background: #fff;
+
+}
+#case_press [data-case='note_3']{
+	background: #fff;
+
+}
+#case_press [data-case='note_4']{
+	width: 200px;
+	background: #fff;
+	color: #000;
+
+}
+#case_press [data-case='note_5']{
+	width: 200px;
+	background: #fff;
+	color: #000;
+
+}
+#case_press [data-case='note_6']{
+	width: 400px;
+	background: #FFFFED;
+	color: #454536;
+}
